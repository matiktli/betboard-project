{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "Template responsible for creation of dynamo-db part",
    "Outputs": {},
    "Parameters": {
        "Environment": {
            "AllowedValues": [
                "prod",
                "sbox",
                "local"
            ],
            "Default": "prod",
            "Description": "Enter environment name, by default prod.",
            "Type": "String"
        }
    },
    "Resources": {
        "todo-name": {
            "Properties": {
                "AttributeDefinitions": [
                    "todo"
                ],
